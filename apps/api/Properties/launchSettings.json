{
	"$schema": "https://json.schemastore.org/launchsettings.json",
	"profiles": {
		"http": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": false,
			"applicationUrl": "http://localhost:5170",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"DB_USER": "${DB_USER:-sa}",
				"DB_PASSWORD": "${DB_PASSWORD:-YourStrong!Passw0rd}",
				"DB_NAME": "${DB_NAME:-DotnetAuthApiSample}"
			}
		},
		"https": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": false,
			"applicationUrl": "https://localhost:7092;http://localhost:5170",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"DB_USER": "${DB_USER:-sa}",
				"DB_PASSWORD": "${DB_PASSWORD:-YourStrong!Passw0rd}",
				"DB_NAME": "${DB_NAME:-DotnetAuthApiSample}"
			}
		}
	}
}
